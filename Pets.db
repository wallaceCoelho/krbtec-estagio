  `breed_id` bigint unsigned NOT NULL,
  `created_at` timestamp NULL DEFAULT NULL,
  `updated_at` timestamp NULL DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `pets_specie_id_foreign` (`specie_id`),
  KEY `pets_breed_id_foreign` (`breed_id`),
  CONSTRAINT `pets_breed_id_foreign` FOREIGN KEY (`breed_id`) REFERENCES `breed` (`id`),
  CONSTRAINT `pets_specie_id_foreign` FOREIGN KEY (`specie_id`) REFERENCES `species` (`id`)
